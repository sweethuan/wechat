/**login.wxss**/
page{
  height: 100vh;
  background: linear-gradient(#1F99FF,white)
}
#form{
  display: block;
  position: relative;
  width: 80%;
  height: 500rpx;
  margin: 0 auto;
  box-shadow: 0 0 5px #333333;
  transform: translateY(50%);
  background: white;
  /* margin-top: 250rpx; */
}
.weui-cell{
  background: transparent ;
}
.weui-cells{
  background: transparent;
}
.loginForm{
  width: 500rpx;
  margin: 0px auto;
  padding-top: 150px;
  position: relative;
  display: block;
}
.loginForm .list{
    width: 100%;
    height: 70rpx;
    border: 1px solid white;
    border-radius: 35rpx;
    position: relative;
    display: flex;
    margin-bottom: 40rpx;
}
.loginForm .list .iconfont{
  display: block;
  font-size: 20px;
  height: 70rpx;
  line-height: 70rpx;
  width:  80rpx;
  text-align: center;
  border-right: 1px solid white;
  color: white;
}
.loginForm .list .cancel{
  position: absolute;
  top: -16px;
  left: 80rpx;
  font-size: 10px;
  color: red;
}
.loginForm .list .login-input{
    width: 410rpx;
    height: 70rpx;
    line-height: 70rpx;
    margin-left: 10rpx;
    text-align: left;
    font-size: 28rpx;
}
.loginForm .submit{
    width: 500rpx;
    height: 70rpx;
    /* border: 1px solid white; */
    border-radius: 35rpx;
    position: relative;
    margin-top: 40rpx;
    letter-spacing: 10px;
    text-indent: 10px;
}