.fl{
  float: left;
}
.fr{
  float: right;
}
.clear::after{
  content: "";
  display: block;
  height: 0;
  clear: both;
  overflow: hidden;
}
@font-face {
  font-family: 'iconfont';  /* project id 1997620 */
  src: url('http://at.alicdn.com/t/font_1997620_bti31l3bucr.eot');
