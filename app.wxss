  url('http://at.alicdn.com/t/font_1997620_bti31l3bucr.woff2') format('woff2'),
  url('http://at.alicdn.com/t/font_1997620_bti31l3bucr.woff') format('woff'),
  url('http://at.alicdn.com/t/font_1997620_bti31l3bucr.ttf') format('truetype'),
  url('http://at.alicdn.com/t/font_1997620_bti31l3bucr.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

